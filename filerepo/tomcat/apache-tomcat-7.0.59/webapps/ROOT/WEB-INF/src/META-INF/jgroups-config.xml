		discard_delivered_msgs="false"
		retransmit_timeout="300,600,1200,2400,4800"
		use_mcast_xmit="false"
	/>
	<pbcast.STABLE
		desired_avg_gossip="50000"
		max_bytes="400000"
		stability_delay="1000"
	/>
	<pbcast.GMS
		join_timeout="7000"
		print_local_addr="true"
		view_bundling="true"
	/>
	<MFC
		max_credits="2000000"
		min_threshold="0.10"
	/>
	<FRAG2
		frag_size="60000"
	/>
	<pbcast.FLUSH />
	<PEER_LOCK />
